unicase/ignorable
unicase/special-casing
unicase/totitle
unicase/tolower
uniwbrk/u8-wordbreaks
unictype/combining-class
unictype/property-soft-dotted
unistr/u8-mbtouc-unsafe
unistr/u8-uctomb
unistr/u8-cpy
uninorm/u8-normalize
stdbool

configure.ac:
gl_LIBUNISTRING_MODULE([0.9.11], [unicase/u8-ct-totitle])

Makefile.am:
if LIBUNISTRING_COMPILE_UNICASE_U8_CT_TOTITLE
lib_SOURCES += unicase/u8-ct-totitle.c
endif

