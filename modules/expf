Description:
expf() function: exponential function.

Files:
lib/expf.c
m4/expf.m4

Depends-on:
math-h
extensions
exp             [test $HAVE_EXPF = 0 || test $REPLACE_EXPF = 1]

configure.ac:
gl_FUNC_EXPF
gl_CONDITIONAL([GL_COND_OBJ_EXPF],
               [test $HAVE_EXPF = 0 || test $REPLACE_EXPF = 1])
gl_MATH_MODULE_INDICATOR([expf])

Makefile.am:
if GL_COND_OBJ_EXPF
lib_SOURCES += expf.c
endif

Include:
<math.h>

Link:
$(EXPF_LIBM)

