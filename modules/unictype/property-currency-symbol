gl_LIBUNISTRING_MODULE([0.9.11], [unictype/property-currency-symbol])

Makefile.am:
if LIBUNISTRING_COMPILE_UNICTYPE_PROPERTY_CURRENCY_SYMBOL
lib_SOURCES += unictype/pr_currency_symbol.c
endif

