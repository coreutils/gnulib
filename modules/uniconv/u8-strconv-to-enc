configure.ac:
gl_LIBUNISTRING_MODULE([0.9], [uniconv/u8-strconv-to-enc])

Makefile.am:
if LIBUNISTRING_COMPILE_UNICONV_U8_STRCONV_TO_ENC
lib_SOURCES += uniconv/u8-strconv-to-enc.c
endif

