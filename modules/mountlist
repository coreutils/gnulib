fopen-gnu
free-posix
getline
open
unlocked-io-internal
bool
stdint-h
stringeq
strstr-simple
xalloc

configure.ac:
gl_MOUNTLIST
gl_PREREQ_MOUNTLIST_EXTRA

Makefile.am:
lib_SOURCES += mountlist.c

Include:
"mountlist.h"

Link:
$(LTLIBINTL) when linking with libtool, $(LIBINTL) otherwise

